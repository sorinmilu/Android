{
    "query": {
      "$query": {
        "lang": "eng"
      },
      "$filter": {
        "forceMaxDataTimeWindow": "31",
        "dataType": ["news", "blog"],
        "isDuplicate": "skipDuplicates"
      }
    },
    "resultType": "articles",
    "articlesSortBy": "date",
    "includeArticleConcepts": true,
    "includeArticleImage": true,
    "articlesPage": 1,
    "articlesCount": 50
  }
  